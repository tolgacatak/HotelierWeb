@model List<TestimonialViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}



 

    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Reference Picture</th>
            <th>Reference Name</th>
            <th>Reference Title</th>
            <th>Delete</th>
            <th>Update</th>
            
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.TestimonialID</th>
                <td>@item.Name</td>
                <td>@item.Title</td>
                <td><a href="/Crew/DeleteCrew/@item.TestimonialID" class="btn mb-1 btn-rounded btn-outline-danger">Delete</a></td>
                <td><a href="/Crew/UpdateCrew/@item.TestimonialID" class="btn mb-1 btn-rounded btn-outline-success">Update</a></td>
                <td><a href="#" class="btn mb-1 btn-rounded btn-outline-dark">Details</a></td>
            </tr>
        }
    </table>
    <a href="/Crew/AddCrew" class="btn mb-1 btn-rounded btn-outline-info"> New Employee Entry </a>


